{
    "name": "yakovlev-vladimir/laravel-mix-twig-extension",
    "license": "MIT",
    "type": "library",
    "description" : "Laravel Mix twig extension",
    "keywords": ["php", "laravel", "twig", "extension", "mix"],
    "autoload": {
        "psr-4": {
            "Stormiix\\Twig\\Extension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stormiix\\Twig\\Extension\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "twig/twig": "~3.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
